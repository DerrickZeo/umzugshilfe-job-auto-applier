{
  "name": "umzugshilfe-job-applier-simple",
  "version": "3.0.0",
  "description": "Lightning-fast job application bot for competitive advantage - simplified for pure speed",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "NODE_ENV=development node app.js",
    "docker:build": "docker build -t umzugshilfe-simple .",
    "docker:run": "docker run -p 3000:3000 --env-file .env umzugshilfe-simple",
    "deploy": "./scripts/deploy-simple.sh",
    "health": "curl -f http://localhost:3000/health || echo 'Health check failed'",
    "stats": "curl -s http://localhost:3000/stats | jq .",
    "test": "curl -X POST -H 'Content-Type: application/json' -d '{\"jobIds\":[\"TEST123\"]}' http://localhost:3000/trigger",
    "setup": "./setup.sh",
    "test:apply": "node scripts/test-apply.js"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.0",
    "iconv-lite": "^0.6.3",
    "imap": "^0.8.19",
    "nodemailer": "^7.0.5",
    "playwright": "^1.54.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "automation",
    "job-application",
    "competitive-advantage",
    "playwright",
    "speed",
    "umzugshilfe"
  ],
  "author": "Speed Bot Team",
  "license": "MIT"
}
